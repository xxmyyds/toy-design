{
  "packageManager": "pnpm@7.12.1",
  "private": "true",
  "engines": {
    "node": ">= 16"
  },
  "keywords": [
    "design",
    "ui",
    "vue",
    "framework"
  ],
  "license": "MIT",
  "author": "xxmyyds",
  "main": "index.js",
  "scripts": {
